{
  "name": "reactnativesimpleapp",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "iosmini": "react-native run-ios --simulator='iPhone 13 mini'",
    "clean-android": "cd android && ./gradlew clean",
    "sim": "open -a simulator",
    "clean-pods": "cd ios && pod deintegrate",
    "clean-ios": "xcodebuild clean build",
    "pod-install": "yarn clean-pods && cd ios && pod install",
    "clean-start": "watchman watch-del-all && rm -rf ./node_modules && yarn cache clean --force && yarn install && yarn clean-android && yarn pod-install && yarn start -- --reset-cache",
    "reset-adb": "adb kill-server && adb start-server",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn lint"
    ]
  },
  "dependencies": {
    "@react-navigation/elements": "^1.3.3",
    "@react-navigation/native": "^6.0.10",
    "@react-navigation/native-stack": "^6.6.2",
    "@reduxjs/toolkit": "^1.8.2",
    "axios": "^0.27.2",
    "react": "^17.0.2",
    "react-native": "^0.68.2",
    "react-native-device-info": "^9.0.2",
    "react-native-flipper": "^0.150.0",
    "react-native-reanimated": "^2.8.0",
    "react-native-safe-area-context": "^4.3.1",
    "react-native-screens": "^3.13.1",
    "react-redux": "^7.2.8",
    "redux-flipper": "^2.0.1",
    "styled-components": "^5.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.11.41",
    "@types/react-native": "^0.67.3",
    "@types/react-test-renderer": "^17.0.1",
    "@types/styled-components": "^5.1.25",
    "@types/styled-components-react-native": "^5.1.3",
    "@typescript-eslint/eslint-plugin": "^5.17.0",
    "@typescript-eslint/parser": "^5.17.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-styled-components": "^2.0.7",
    "eslint": "7.32.0",
    "husky": "^8.0.1",
    "jest": "^26.6.3",
    "lint-staged": "^13.0.2",
    "metro-react-native-babel-preset": "^0.67.0",
    "prettier": "^2.7.1",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17",
    "styled-components": "^5"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
